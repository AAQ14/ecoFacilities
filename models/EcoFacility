const mongoose = require("mongoose")
const {model, Schema} = mongoose

const ecoFacilitySchema = new Schema({
    name: {
        type: String,
        require: true
    },
    category: {
        type: String,
        require : true, 
        enum : []
    },
    descrtiption:{
        type: String
    },
    houseNumber:{
        type: String
    },
    streetName:{
        type: String
    }, 
    town: String ,
    contributor: String,
    status: String
})

const EcoFacility = model("EcoFacility", ecoFacilitySchema)

module.exports = EcoFacility